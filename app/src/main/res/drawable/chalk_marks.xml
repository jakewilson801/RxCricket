<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/first_strike">
        <rotate android:fromDegrees="45">
            <shape android:shape="line">
                <stroke
                    android:width="1dp"
                    android:color="@color/strike_one" />
                <solid android:color="@color/strike_one" />
            </shape>
        </rotate>
    </item>
    <item android:id="@+id/second_strike">
        <rotate android:fromDegrees="135">
            <shape android:shape="line">
                <stroke
                    android:width="1dp"
                    android:color="@color/strike_one" />
                <solid android:color="@color/strike_one" />
            </shape>
        </rotate>
    </item>

    <item android:id="@+id/third_strike">
        <shape android:shape="oval">
            <gradient android:startColor="@color/strike_one" android:endColor="@color/strike_one"
                android:angle="270"/>
        </shape>
    </item>
</layer-list>
